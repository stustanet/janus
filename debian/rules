#!/usr/bin/make -f

%:
	dh $@ --with python3

override_dh_systemd_enable:
	dh_systemd_enable --name=ssh-janus
	dh_systemd_enable --name=ssh-janus-update

override_dh_installsystemd:
	dh_installsystemd --name=ssh-janus
	dh_installsystemd --name=ssh-janus-update

override_dh_systemd_start:
	dh_systemd_start --name=ssh-janus
	dh_systemd_start --name=ssh-janus-update
